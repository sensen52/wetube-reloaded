extends base.pug

block content
    form(method="POST")
        input(name="name", type="text", required:true,placeholder="이름")
        input(name="email", type="email", required:true,placeholder="이메일")
        input(name="username", type="text", required:true,placeholder="유저이름")
        input(name="password", type="text", required:true,placeholder="비밀번호")
        input(name="location", type="text", required:true,placeholder="주소")
        input(type="submit", value="Join")